{
  "expo": {
    "name": "acnh",
    "slug": "acnh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iome.acnh"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.iome.acnh"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "json",
        "mjs",
        "cjs"
      ],
      "nodeModulesPath": "node_modules"
    },
    "plugins": [
      "expo-font"
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "your-organization",
            "project": "your-project",
            "authToken": "your-auth-token"
          }
        }
      ]
    },
    "extra": {
      "expo": {
        "fonts": [
          {
            "asset": "assets/fonts/FOT-RodinBokutoh Pro EB.otf"
          }
        ]
      },
      "eas": {
        "projectId": "8f87a414-9b08-4a13-abc6-1832be295331"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8f87a414-9b08-4a13-abc6-1832be295331"
    }
  }
}
